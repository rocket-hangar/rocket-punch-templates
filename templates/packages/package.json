{
  "name": "packages-template",
  "version": "1.0.0",
  "scripts": {
    "build": "rocket-punch build",
    "publish": "rocket-punch publish",
    "view": "rocket-punch view",
    "doctor": "rocket-punch doctor",
    "test": "jest --colors",
    "coverage": "jest --coverage",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/{**/*,*}/*.{ts,tsx}": [
      "jest --colors --findRelatedTests"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "rocket-punch": "^2.4.1",
    "typescript": "^4.2.3"
  }
}
